{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-extra-semi": "error",
        "semi": [2, "always"],
        "space-before-function-paren": "off"
    }
}
